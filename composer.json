{
    "name": "sbs/yii2-transliterator-helper",
    "description": "The transliterator helper for the Yii 2 framework",
    "keywords": [
        "extension",
        "transliterator",
        "helper",
        "yii"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/koftikes/yii2-transliterator-helper",
    "authors": [
        {
            "name": "Konstantin Litvinov",
            "email": "koftikes@gmail.com",
            "homepage": "http://sbsweb.ru"
        },
        {
            "name": "Antonio Ramirez",
            "email": "ramirez.cobos@gmail.com",
            "homepage": "http://2amigos.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "sbs\\": "src",
            "tests\\": "tests"
        }
    },
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.14"
    }
}
